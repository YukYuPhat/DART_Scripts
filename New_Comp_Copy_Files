    Copy-Item -Path I:\Verint\clientlicense.licx -Destination C:\Temp\ |out-null

    #Start-Process C:\Temp\Op-Center_Setup_4.3.15.26551.exe |out-null

    echo "Server name: DPAPPVRNT1\SQLEXPRESS"
    echo "Username: sa"
    echo "Password: search for: DPAPPVRNT1 SA PW"
    echo "Client Serial Number: 06ADE-8F0B7-A54AF-D87C1-121EA-FC048"
    echo "Client License file has been copied to the C:\Temp folder"


    Read-Host "Op Center is installed"

    Copy-Item -Path I:\Verint\Vid-Center_Setup_4.3.15.26551.exe -Destination C:\Temp\ |out-null

    read-host 'Ops-Center and Vid-Center done'
}

