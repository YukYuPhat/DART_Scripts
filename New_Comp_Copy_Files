#create Temp folder and copy script into folder

#copy script to new computer from I: drive

#Copy cisco umbrella and dart installers to temp

$NewComputer = read-host "Type yes to install onto a new computer."

if ($NewComputer -eq 'yes'){

    Copy-Item -Path I:\Cisco\AnyConnect\anyconnect-win-4.7.00136-predeploy-k9\anyconnect-win-4.7.00136-umbrella-predeploy-k9.msi -Destination C:\Temp\ |out-null

    Copy-Item -Path I:\Cisco\AnyConnect\anyconnect-win-4.7.00136-predeploy-k9\anyconnect-win-4.7.00136-dart-predeploy-k9.msi -Destination C:\Temp\ |out-null

    msiexec.exe /i anyconnect-win-4.7.00136-umbrella-predeploy-k9.msi |out-null
    read-host “Press ENTER to install Dart Cisco files...”

    msiexec.exe /i anyconnect-win-4.7.00136-dart-predeploy-k9.msi |out-null

    echo 'cisco done'

    Copy-Item -Path I:\Drivers\Realtek-High-Definition-Audio-Driver_5CH91_WIN_6.0.8960.1_A02.exe -Destination C:\Temp\ |out-null
    Start-Process C\Temp\Realtek-High-Definition-Audio-Driver_5CH91_WIN_6.0.8960.1_A02.exe |out-null

    echo 'Sound Driver done'

    Copy-Item -Path I:\Microsoft\Teams_windows_x64.exe -Destination C:\Temp\
    echo 'Teams Copied'
}

$Trapeze = read-host “Type yes to install Trapeze files...”

if ($Trapeze -eq 'yes'){

    #copy  I:\Trapeze\TM17 Software to temp folder

    Copy-Item -Path I:\Trapeze\'TM17 Software' -Destination C:\Temp\ -Recurse |out-null


    Start-Process C:\Temp\'TM17 Software'\'Des Moines_DART_22S2000-017 Gen II Release v17.0.0.20 (08-04-2017)'\'Visual C++ Redistributable'\vcredist_x86.exe |out-null

    read-host “Press ENTER to install DhtmlEd files...”

    msiexec.exe /i C:\Temp\'TM17 Software'\'Des Moines_DART_22S2000-017 Gen II Release v17.0.0.20 (08-04-2017)'\'22S2000-017 Dispatch (08-04-2017)'\DhtmlEd.msi |out-null

    read-host “Press ENTER to install TMprerequisites files...”
    echo "insert DPAPPAVL3 into application server DNS"


    msiexec.exe /i C:\Temp\'TM17 Software'\'Des Moines_DART_22S2000-017 Gen II Release v17.0.0.20 (08-04-2017)'\'22S2000-017 Dispatch (08-04-2017)'\TMPrerequisites.msi |out-null

    read-host “Press ENTER to install TMDispatchWorkstation files...”
    echo "insert DPAPPAVL3 into machine name"

    msiexec.exe /i C:\Temp\'TM17 Software'\'Des Moines_DART_22S2000-017 Gen II Release v17.0.0.20 (08-04-2017)'\'22S2000-017 Dispatch (08-04-2017)'\TMDispatchWorkstation.msi |out-null

    read-host “Press ENTER to install TM Des Moines Map Data files...”

    msiexec.exe /i C:\Temp\'TM17 Software'\'TM Des Moines Map Data (Lay).msi' |out-null

    read-host “Press ENTER to Copy AppData files...”

    read-host “Stop Service TMAppDataService and then press ENTER”

    #Stop-Service -Name "TMAppDataService"

    Copy-Item -Path C:\Temp\'TM17 Software'\'Des Moines_DART_22S2000-017 Gen II Release v17.0.11.1 (03-07-2018)NR'\WorkstationInstallForUsers\* -Destination C:\TransitMaster\ |out-null

    read-host “Start Service TMAppDataService and then press ENTER”

    #Start-Service -Name "TMAppDataService"

    echo "open TM Bus Ops and enter the following into map download configuration"
    echo "IP Address: 10.1.100.16"
    echo "FTP Login: SVCTMFTP"
    echo "Password in Keepass"
    echo "Remote Map Location: \Mapfiles"

    read-host “After verifing Bus Ops press ENTER”
    echo 'trapeze done'
} 






$Touch = Read-Host "Install TouchPoint? yes or No"
if ($Touch -eq 'yes'){

    # copy enghouse touchpoint install and setup files

    Copy-Item -Path I:\Enghouse\TouchPointInstall.reg -Destination C:\Temp\  |out-null

    echo "open registry and add TouchPointInstall.reg"
    read-host "Press enter after closing registry"



    Copy-Item -Path I:\Enghouse\TouchPointSetup.exe -Destination C:\Temp\ |out-null

    Start-Process C:\Temp\TouchPointSetup.exe |out-null


    read-host 'touch point done'}

